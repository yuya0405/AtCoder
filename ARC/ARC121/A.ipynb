   "source": [
    "N = int(input())\n",
    "XY = [list(map(int, input().split())) for _ in range(N)]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": []
  },
  {
   "cell_type": "code",
   "execution_count": 18,
   "metadata": {},
   "outputs": [],
   "source": [
    "# TLE\n",
    "\n",
    "from itertools import combinations\n",
    "d = []\n",
    "for c in combinations(range(N), 2):\n",
    "    d.append(max(abs(XY[c[0]][0]-XY[c[1]][0]), abs(XY[c[0]][1]-XY[c[1]][1])))\n",
    "d.sort()\n",
    "\n",
    "print (d[-2])"
   ]
  },
