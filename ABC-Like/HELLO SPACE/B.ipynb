    "N, D, H = map(int, input().split())\n",
    "dh = [list(map(int, input().split())) for _ in range(N)]\n",
    "\n",
    "katamuki = []\n",
    "for i in dh:\n",
    "    tmp = (H - i[1])/(D - i[0])\n",
    "    katamuki.append(tmp)\n",
    "min_katamuki = min(katamuki)\n",
    "\n",
    "y = min_katamuki * (-1 * D) + H\n",
    "\n",
    "if y > 0:\n",
    "    print (y)\n",
    "else:\n",
    "    print (0)"
   ]
  }
