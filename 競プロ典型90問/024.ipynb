   "source": [
    "N, K = map(int, input().split())\n",
    "A = list(map(int, input().split()))\n",
    "B = list(map(int, input().split()))\n",
    "\n",
    "diff = 0\n",
    "for i in range(N):\n",
    "    diff += abs(A[i] - B[i])\n",
    "if diff > K:\n",
    "    print (\"No\")\n",
    "elif diff % 2 == K % 2:\n",
    "    print (\"Yes\")\n",
    "else:\n",
    "    print (\"No\")"
   ]
  }
