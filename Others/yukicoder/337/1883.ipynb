   "source": [
    "abc = int(input())\n",
    "\n",
    "a = abc // 100\n",
    "\n",
    "b = (abc - a*100) // 10\n",
    "\n",
    "c = abc - a*100 - b*10\n",
    "\n",
    "M = max(max(a, b), c)\n",
    "\n",
    "m = min(min(a, b), c)\n",
    "\n",
    "print (M-m)"
   ]
