   "source": [
    "N = int(input())\n",
    "H = list(map(int, input().split()))\n",
    "\n",
    "H.insert(0, 0)\n",
    "\n",
    "d = [10**100] * (N+1)\n",
    "d[0] = 0\n",
    "d[1] = 0\n",
    "d[2] = abs(H[2]-H[1])\n",
    "\n",
    "for i in range(3, N+1):\n",
    "    d[i] = min(d[i-2]+abs(H[i]-H[i-2]), d[i-1]+abs(H[i]-H[i-1]))\n",
    "\n",
    "print (d[N])"
   ]
