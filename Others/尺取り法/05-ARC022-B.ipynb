    "from collections import deque"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 39,
   "id": "01484895-2b4b-43ce-86c5-9967bfa50d7a",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "3\n"
     ]
    }
   ],
   "source": [
    "ans = 0\n",
    "que = deque()\n",
    "for a in A:\n",
    "    while len(que) >= 1 and a in que:\n",
    "        que.popleft()\n",
    "        \n",
    "    que.append(a)\n",
    "    ans = max(ans, len(que))\n",
    "\n",
    "print (ans)"
   ]
