      "1\n",
      "1\n",
      "1\n"
     ]
    }
   ],
   "source": [
    "from collections import deque\n",
    "\n",
    "dq = deque()\n",
    "for t, x in TX:\n",
    "    if t==1:\n",
    "        dq.appendleft(x)\n",
    "    elif t==2:\n",
    "        dq.append(x)\n",
    "    elif t==3:\n",
    "        print (dq[x-1])"
   ]
