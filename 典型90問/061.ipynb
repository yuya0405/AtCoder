   "source": [
    "Q = int(input())\n",
    "TX = [list(map(int, input().split())) for _ in range(Q)]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "id": "ee83b4c5-814d-4142-8dca-b48e8a26a853",
