   "source": [
    "import itertools\n",
    "\n",
    "ans = 0\n",
    "COMB = itertools.combinations(A, 5)\n",
    "for comb in COMB:\n",
    "    tmp = 1\n",
    "    for c in comb:\n",
    "        tmp *= c\n",
    "        tmp %= P\n",
    "    \n",
    "    if tmp == Q:\n",
    "        ans += 1\n",
    "print (ans)"
