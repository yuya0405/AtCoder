   ],
   "source": [
    "dp = [0]*(N+1)\n",
    "dp[0] = 1\n",
    "\n",
    "for i in range(N+1):\n",
    "    dp[i] += dp[i-1]\n",
    "    dp[i] %= 10**9+7\n",
    "    if i-L >= 0:\n",
    "        dp[i] += dp[i-L]\n",
    "        dp[i] %= 10**9+7\n",
    "print (dp[N])"
   ]
