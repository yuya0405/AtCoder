   "source": [
    "T = int(input())\n",
    "num_list = [list(map(int, input().split())) for _ in range(T)]\n",
    "\n",
    "for L, R in num_list:\n",
    "    cnt = 0\n",
    "    for i in range(L, R+1):\n",
    "        for j in range(L, R+1):\n",
    "            if L <= i-j and i-j <= R:\n",
    "                cnt += 1\n",
    "    print (cnt)"
   ]
