   "source": [
    "A, B, C, X, Y = map(int, input().split())\n",
    "\n",
    "# その1\n",
    "maximun = max(X, Y)\n",
    "cost = C*2*maximun\n",
    "\n",
    "# その2\n",
    "minimum = min(X, Y)\n",
    "for i in range(minimum+1):\n",
    "    cost_tmp = A*(X-i)+B*(Y-i)+C*2*i\n",
    "    if cost_tmp < cost:\n",
    "        cost = cost_tmp\n",
    "print(cost)"
   ]
  }
