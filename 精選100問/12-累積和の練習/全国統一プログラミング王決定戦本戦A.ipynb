   "source": [
    "N = int(input())\n",
    "A = list(map(int, input().split()))\n",
    "\n",
    "sum = [0]\n",
    "for i in range(N):\n",
    "    sum.append(sum[i] + A[i])\n",
    "\n",
    "for k in range(1, N+1):\n",
    "    ans = 0\n",
    "    for i in range(N-k+1):\n",
    "        ans = max(ans, sum[i+k] - sum[i])\n",
    "    print (ans)"
   ]
