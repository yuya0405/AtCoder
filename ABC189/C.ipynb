   "source": [
    "N = int(input())\n",
    "l = list(map(int, input().split()))\n",
    "\n",
    "ans = 0\n",
    "for i in range(1, N+1):\n",
    "    for j in range(i, N+1):\n",
    "        num = min(l[i-1:j])\n",
    "        if ans < (j-i+1)*num:\n",
    "            ans = (j-i+1)*num\n",
    "print(ans)"
   ]
