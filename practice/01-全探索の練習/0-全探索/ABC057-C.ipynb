    "import math\n",
    "\n",
    "N = int(input())\n",
    "\n",
    "ans = 10\n",
    "for A in range(1, int(math.sqrt(N)+1)):\n",
    "    if N%A == 0:\n",
    "        B = int(N/A)\n",
    "        if len(str(B)) <= ans:\n",
    "            ans = len(str(B))\n",
    "print (ans)"
