   "source": [
    "K = int(input())\n",
    "\n",
    "import math\n",
    "\n",
    "ans = 0\n",
    "for a in range(1, K+1):\n",
    "    for b in range(1, K+1):\n",
    "        for c in range(1, K+1):\n",
    "            ans += math.gcd(math.gcd(a,b), c)\n",
    "print (ans)"
