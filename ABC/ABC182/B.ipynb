    "A = list(map(int, input().split()))\n",
    "\n",
    "import numpy as np\n",
    "\n",
    "count = 0\n",
    "for i in range(2, max(A)+1):\n",
    "    tmp = list(np.array(A)%i).count(0)\n",
    "    if tmp >= count:\n",
    "        count = tmp\n",
    "        ans = i\n",
    "print (ans)"
