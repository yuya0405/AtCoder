   "source": [
    "H, W = map(int, input().split())\n",
    "S = [input() for _ in range(H)]\n",
    "\n",
    "pos = []\n",
    "for i, line in enumerate(S):\n",
    "    for j, s in enumerate(line):\n",
    "        if s == \"o\":\n",
    "            pos.append([i, j])\n",
    "\n",
    "dy = abs(pos[0][0] - pos[1][0])\n",
    "dx = abs(pos[0][1] - pos[1][1])\n",
    "print (dx + dy)"
   ]
