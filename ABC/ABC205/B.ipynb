    "N = int(input())\n",
    "A = list(map(int, input().split()))\n",
    "\n",
    "flag = True\n",
    "for n in range(1, N+1):\n",
    "    if n not in A:\n",
    "        flag = False\n",
    "\n",
    "if flag == True:\n",
    "    print (\"Yes\")\n",
    "else:\n",
    "    print (\"No\")"
   ]
