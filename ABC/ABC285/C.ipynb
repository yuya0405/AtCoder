   "source": [
    "def alpha2num(alpha):\n",
    "    num=0\n",
    "    for index, item in enumerate(list(alpha)):\n",
    "        num += pow(26,len(alpha)-index-1)*(ord(item)-ord('A')+1)\n",
    "    return num"
   ]
  },
