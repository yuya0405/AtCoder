   "source": [
    "N = int(input())\n",
    "AB = [list(map(int, input().split())) for _ in range(N-1)]\n",
    "\n",
    "count = []\n",
    "for ab in AB:\n",
    "    for _ in ab:\n",
    "        count.append(_)\n",
    "\n",
    "from collections import Counter\n",
    "\n",
    "for c in Counter(count).values():\n",
    "    if c == N-1:\n",
    "        print (\"Yes\")\n",
    "        exit()\n",
    "print (\"No\")"
   ]
