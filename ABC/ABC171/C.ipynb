   "source": [
    "N = int(input())\n",
    "\n",
    "ans = []\n",
    "while(1):\n",
    "    tmp = N % 26\n",
    "    if tmp == 0:\n",
    "        ans.append(\"z\")\n",
    "    else:\n",
    "        ans.append(chr(ord('a') + tmp -1))\n",
    "    N //= 26\n",
    "    if N == 0:\n",
    "        break\n",
    "\n",
    "print (\"\".join(ans[::-1]))"
   ]
