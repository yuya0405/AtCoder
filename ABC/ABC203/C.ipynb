    "AB = [list(map(int, input().split())) for _ in range(N)]\n",
    "\n",
    "AB.sort()\n",
    "\n",
    "money = K\n",
    "for ab in AB:\n",
    "    if ab[0] <= money:\n",
    "        money += ab[1]\n",
    "print (money)"
