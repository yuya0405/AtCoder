   "source": [
    "N, X = map(int, input().split())\n",
    "A = list(map(int, input().split()))\n",
    "\n",
    "sum = 0\n",
    "for i, a in enumerate(A):\n",
    "    if i % 2 == 1:\n",
    "        sum += a-1\n",
    "    else:\n",
    "        sum += a\n",
    "if sum <= X:\n",
    "    print (\"Yes\")\n",
    "else:\n",
    "    print (\"No\")"
   ]
