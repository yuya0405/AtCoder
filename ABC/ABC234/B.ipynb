   "source": [
    "N = int(input())\n",
    "XY = [list(map(int, input().split())) for _ in range(N)]\n",
    "\n",
    "ans = -1.0\n",
    "for i in range(0, N):\n",
    "    for j in range(i+1, N):\n",
    "        ans = max(ans, ((XY[i][0] - XY[j][0])**2 + (XY[i][1] - XY[j][1])**2)**0.5)\n",
    "print (ans)"
   ]
