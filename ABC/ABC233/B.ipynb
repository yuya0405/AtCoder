   "source": [
    "L, R = map(int, input().split())\n",
    "S = input()\n",
    "\n",
    "ans = \"\"\n",
    "ans += S[:L-1]\n",
    "\n",
    "for i in reversed(S[L-1:R]):\n",
    "    ans += i\n",
    "\n",
    "ans += S[R:]\n",
    "\n",
    "print (ans)"
   ]
