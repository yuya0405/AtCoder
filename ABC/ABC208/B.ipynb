   "source": [
    "P = int(input())\n",
    "\n",
    "import math\n",
    "\n",
    "V = [math.factorial(i) for i in range(1, 11)]\n",
    "\n",
    "ans = 0\n",
    "for i in range(9, -1, -1):\n",
    "    t = min(P//V[i], 100)\n",
    "    P -= t*V[i]\n",
    "    ans += t\n",
    "print (ans)"
   ]
  }
