    "S = input()\n",
    "\n",
    "flag = True\n",
    "for num, s in enumerate(S):\n",
    "    num += 1\n",
    "    if num%2 == 1 and s < 'a':\n",
    "        flag = False\n",
    "        break\n",
    "    if num%2 == 0 and s > 'Z':\n",
    "        flag = False\n",
    "        break\n",
    "\n",
    "if flag == True:\n",
    "    print (\"Yes\")\n",
    "if flag == False:\n",
    "    print (\"No\")"
   ]
  }
