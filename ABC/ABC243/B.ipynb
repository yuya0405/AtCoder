   "source": [
    "N = int(input())\n",
    "A = list(map(int, input().split()))\n",
    "B = list(map(int, input().split()))\n",
    "\n",
    "ans = 0\n",
    "for i in range(N):\n",
    "    if A[i] == B[i]:\n",
    "        ans += 1\n",
    "print (ans)\n",
    "\n",
    "A_set = set(A)\n",
    "B_set = set(B)\n",
    "\n",
    "print (len(A_set & B_set) - ans)"
   ]
