   "source": [
    "A, B, C, D = map(int, input().split())\n",
    "\n",
    "mizu = A\n",
    "aka = 0\n",
    "cnt = 0\n",
    "\n",
    "flag = True\n",
    "while(flag):\n",
    "    if mizu <= aka * D:\n",
    "        print (cnt)\n",
    "        break\n",
    "    cnt += 1\n",
    "    mizu += B\n",
    "    aka += C\n",
    "    if cnt >= 10**5+1:\n",
    "        print (-1)\n",
    "        break"
   ]
