    "N = int(input())\n",
    "APX = []\n",
    "for i in range(N):\n",
    "    APX.append(list(map(int,input().split())))\n",
    "\n",
    "ans = 10**9+1\n",
    "for apx in APX:\n",
    "    if apx[2] - apx[0] > 0 and ans > apx[1]:\n",
    "        ans = apx[1]\n",
    "if ans == 10**9+1:\n",
    "    print (-1)\n",
    "else:\n",
    "    print (ans)"
