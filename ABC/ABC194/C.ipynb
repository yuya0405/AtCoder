    "N = int(input())\n",
    "A = list(map(int, input().split()))\n",
    "\n",
    "ans = 0\n",
    "for i in range(N):\n",
    "    ans += A[i]**2\n",
    "ans *= N\n",
    "\n",
    "tmp = 0\n",
    "for i in range(N):\n",
    "    tmp += A[i]\n",
    "ans -= tmp**2\n",
    "\n",
    "print (ans)"
   ]
