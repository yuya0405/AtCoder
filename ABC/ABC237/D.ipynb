   "source": [
    "N = int(input())\n",
    "S = input()\n",
    "\n",
    "from collections import deque\n",
    "\n",
    "A = deque([N])\n",
    "\n",
    "for i, s in enumerate(reversed(S)):\n",
    "    if s == \"R\":\n",
    "        A.appendleft(N-i-1)\n",
    "    else:\n",
    "        A.append(N-i-1)\n",
    "\n",
    "print (' '.join(map(str, A)))"
   ]
