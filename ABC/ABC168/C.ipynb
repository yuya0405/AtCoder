    "import math\n",
    "\n",
    "M_theta = M/60*2*math.pi\n",
    "H_theta = math.pi/6*H + M/60*math.pi/6\n",
    "theta = abs(M_theta - H_theta)\n",
    "\n",
    "X = math.sqrt(A**2+B**2-2*A*B*math.cos(theta))\n",
    "print (X)"
